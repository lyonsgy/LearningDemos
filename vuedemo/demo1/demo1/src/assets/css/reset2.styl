//  resize:none
//  -webkit-appearance: none

input[type="search"]::-webkit-search-cancel-button
  display: none

img
  max-width 100%
  vertical-align:top
  border:none
  overflow:hidden
.bullet
  display block
  width .2rem
  height .2rem
  margin-right .25rem
  border-radius 50%
  background #e8ccd1
  &.bulletOn {
    background -webkit-linear-gradient(0deg, #eb5b5b, #e51f44)
  }
.bulletWhite
  display block
  width .14rem
  height .14rem
  margin-right .18rem
  border-radius 50%
  background rgba(255,255,255,.5)
  &.bulletWhiteOn {
    background #ffffff
  }
.swiper-wrapper{
  height 100%
}







